{
  "build": {
    "env": {
      "NPM_CONFIG_FROZEN_LOCKFILE": "false"
    }
  },
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "http://13.48.23.117:5001/:path*"
    }
  ],
  "headers": [
    {
      "source": "/assets/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ]
}

